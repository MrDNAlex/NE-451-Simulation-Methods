units metal         # Use Metal Units (Ã…, eV, ps, g/mol)
dimension 3         # 3D Simulation
boundary p p p      # Preiodic Boundaryies in X Y Z
atom_style atomic   # Only simulate Atoms, No Bonds, No charges

# Read the "XYZ" File
read_data Fe.data
