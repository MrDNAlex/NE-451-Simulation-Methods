# Assign the Group to Argon and Assign it's mass
group Ar type 1
2048 atoms in group Ar
mass 1 1.0

# Define the Potential of the Simulation
pair_style lj/cut 2.5
pair_coeff 1 1 1.0 1.0

# Set the initial velocity
velocity all create ${temp} 54343
velocity all create 1.5 54343

# Define the Thermodynamics Output
thermo 100
thermo_style custom step temp press vol etotal
timestep 0.005

# Compute the RDF
compute RDF all rdf ${rdf_bins}
compute RDF all rdf 50

# Run the Simulation
fix integrate1 all npt temp ${temp} ${temp} 1 iso 5 5 1
fix integrate1 all npt temp 1.5 ${temp} 1 iso 5 5 1
fix integrate1 all npt temp 1.5 1.5 1 iso 5 5 1
fix 1 all ave/time 100 10 1000 c_RDF[*] file RDF_Pre_Sim${sim_run}.data mode vector
fix 1 all ave/time 100 10 1000 c_RDF[*] file RDF_Pre_Sim1.data mode vector


# Run the Simulation
run 20000
